[core]
    editor = nvim
[user]
	email = codyfbender@gmail.com
	name = Cody Bender
[push]
	default = tracking
[pull]
	rebase = true
[alias]
	co = checkout
	st = status
	cm = commit -m
	ac = "!f() { git add -A && git commit -m \"$@\";}; f"
	acp = "!f() { git add -A && git commit -m \"$@\" && git push; }; f"
[color]
    diff = auto
    status = auto
    branch = auto
[color "status"]
    added = green
    changed = blue
    untracked = red 
[color "diff"]
    meta = yellow
    old = red 
    new = green
