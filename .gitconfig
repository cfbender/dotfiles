[core]
    editor = nvim
[user]
	email = codyfbender@gmail.com
	name = Cody Bender
[push]
	default = tracking
[pull]
	rebase = true
[alias]
	co = checkout
  dt = difftool
	st = status
	cm = commit -m
	ac = "!f() { git add -A && git commit -m \"$@\";}; f"
	acp = "!f() { git add -A && git commit -m \"$@\" && git push; }; f"
[diff]
	tool = meld
[merge]
	tool = meld
 	conflictstyle = diff3
